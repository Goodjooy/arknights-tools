    {
      "ch": "整合运动成员",
      "zh": "啊——！！",
      "en": "Ahh——!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "怎，怎么会！！我不想死......！",
      "en": "How, how could this be!! I don’t want to die……!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "用法术击毁建筑做掩体！！",
      "en": "Use spells to destroy the building for cover!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "这里不是天灾中心区！能行！！",
      "en": "This is not the center of the Calamity! It would work!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "临光",
      "zh": "重装干员！",
      "en": "Shield operators!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "临光",
      "zh": "保护术师！！",
      "en": "Protect the casters!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "临光",
      "zh": "——！！",
      "en": "——!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "罗德岛精英干员",
      "zh": "有陨石撞击了临街建筑！卧倒！！",
      "en": "A meteor hit the building by the street! Get down!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "有坠岩——",
      "en": "Falling rocks incoming——",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "——博士？！",
      "en": "——Doctor?!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "PLAYER",
      "zh": "要求罗德岛术师射击坠石",
      "en": "Command Rhodes Island casters to shoot the falling rocks",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "怎么————",
      "en": "What the————",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "PLAYER",
      "zh": "命令整合运动术师",
      "en": "Command the Reunion Movement casters",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "......"
    },
    {
      "ch": "整合运动成员",
      "zh": "打爆它，打爆它啊，啊啊————！！",
      "en": "Blast it, blast it, ahhh————!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "交叉火力成功击碎了岩体？！",
      "en": "The crossfire shattered the falling rocks?!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "临光",
      "zh": "博士！！",
      "en": "Doctor!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "临光",
      "zh": "咳哈......！",
      "en": "Cough……!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "临光",
      "zh": "下次我可不一定能救到你！！",
      "en": "I might not be able to save you next time!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "整合运动伤亡惨重——！",
      "en": "Reunion Movement suffered heavy losses——!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "如果不击碎落石的话，我们也要被波及——！",
      "en": "If the falling rocks weren’t destroyed, we’d be affected too——!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "啊啊啊！！罗德岛！！别想走！！去死！！！",
      "en": "Ahhh!! Rhodes Island!! Don’t you try to escape!! Die!!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "——整合运动向我们发动进攻了？",
      "en": "——Reunion Movement is attacking us?",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "临光",
      "zh": "这些混蛋，是被天灾逼疯了吗？！！",
      "en": "These bastards, did the Calamity just drive them crazy?!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "PLAYER",
      "zh": "命令临光带队向下城区出口移动",
      "en": "Command Nearl to lead the team to the lower downtown exit",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "临光",
      "zh": "——明白！",
      "en": "——Understood!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "PLAYER",
      "zh": "指挥战斗",
      "en": "Command the battle",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "......！",
      "en": "……!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "交给你了，博士！！",
      "en": "I’ll leave it to you, Doctor!!",
      "jp": "",
      "kr": ""
    }
  ]
