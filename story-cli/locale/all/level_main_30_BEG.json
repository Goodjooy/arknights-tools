  "messages": [
    {
      "zh": "第三十关（前）",
      "en": "Level 30(beginning)",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "碎骨",
      "zh": "............！",
      "en": "............!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动",
      "zh": "咕——！糟，糟了！",
      "en": "Ahhh——! This, this is bad!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动",
      "zh": "呃——",
      "en": "Urghh——",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动",
      "zh": "他们冲破防线了！",
      "en": "They broke through our defenses!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动",
      "zh": "碎骨！你撤退吧！我们还顶得住！",
      "en": "Bone Breaker! You retreat now! We still can hold on!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动",
      "zh": "你也走！快走！你还年轻！！跟着碎骨走！",
      "en": "You should go too! Hurry! You are still young!! Go with Bone Breaker!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "我......我......安东，我不能————",
      "en": "I…...I…...Anton,I can’t——",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "——快走啊！快！",
      "en": "——Run, hurry! Run!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "碎骨——————！",
      "en": "Bone Breaker————!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "碎骨",
      "zh": "我不会走的。",
      "en": "I will not leave.",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "碎骨",
      "zh": "我必须......",
      "en": "I have to……",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "碎骨",
      "zh": "我必须————！！",
      "en": "I have to————!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "碎骨",
      "zh": "————————————",
      "en": "————————————"
    },
    {
      "ch": "陈",
      "zh": "别让敌人重整队形！",
      "en": "Don’t let the enemies reform!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "陈",
      "zh": "星熊，走！",
      "en": "Hoshiguma, let’s go!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "星熊",
      "zh": "不会给他们机会的！",
      "en": "We won’t give them the chance!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "近卫局队员",
      "zh": "小心敌人的冲锋！",
      "en": "Be careful of the enemies’ charges!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "PLAYER",
      "zh": "指挥狙击小组进行牵制射击",
      "en": "Command the sniper squad for suppressive fire",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "罗德岛——准备——罗德岛......",
      "en": "Rhodes Island——prepare to——Rhodes Island…...",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "我，我不能......我......",
      "en": "I, I can’t…….I…...",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "PLAYER",
      "zh": "别忘了你该做的事",
      "en": "Don’t forget what you should do",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "碎骨",
      "zh": "............"
    },
    {
      "ch": "阿米娅",
      "zh": "......我......",
      "en": "......I…….",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "............"
    },
    {
      "ch": "阿米娅",
      "zh": "————！",
      "en": "",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "博士——",
      "en": "Doctor….",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "指挥就——拜托您了。",
      "en": "The commanding——I’ll leave it to you.",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "PLAYER",
      "zh": "鼓励阿米娅去取得胜利",
      "en": "Encourage Amiya to achieve victory",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "......嗯。",
      "en": ".......Yeah.",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "罗德岛的各位，这场战斗，让我们......结束它吧。",
      "en": "Everyone from Rhodes Island, let’s……put an end to this.",
      "jp": "",
      "kr": ""
    }
  ]
}
