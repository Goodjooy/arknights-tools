{
  "messages": [
    {
      "zh": "第一关（前）",
      "en": "Level 1 (Pre-operation)",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "准备出发吧——",
      "en": "Let’s get ready to leave ",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "——等等。",
      "en": "—Wait",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "医疗干员",
      "zh": "怎么了？",
      "en": "What?",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "......刚才我们清理掉的，不是唯一的敌人。",
      "en": "......The enemies we just took care of, they’re not the only ones here",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "？？？",
      "zh": "............."
    },
    {
      "ch": "？？？",
      "zh": "————"
    },
    {
      "ch": "阿米娅",
      "zh": "又是整合运动吗！",
      "en": "It’s more of the Reunion Movement!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "医疗干员",
      "zh": "那是谁？！",
      "en": "Who’s there?!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "医疗干员",
      "zh": "她......她和其他那些整合运动......不太一样......",
      "en": "She….She’s different from.… the others….",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "糟糕......他们的数量和刚才相比多上太多了！",
      "en": "This is bad….their numbers far surpass what the ones we just dealt with",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "？？？",
      "zh": "——————"
    },
    {
      "ch": "？？？",
      "zh": "撕碎他们。",
      "en": "Crush them",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "是。",
      "en": "Yes",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "罗德岛干员",
      "zh": "狙击小队，我们该瞄准她的落脚点！",
      "en": "Sniper squad, aim for the footholds!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "不好！后方隐藏着整合运动！",
      "en": "Not good! There are enemies behind us!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "——是埋伏！别暴露到掩体之外！",
      "en": "—it’s an ambush! Keep yourself in cover!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "PLAYER",
      "zh": "危险！",
      "en": "Look out!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "快躲开！！",
      "en": "Get down!!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "医疗干员",
      "zh": "啊！",
      "en": "Ah!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "医疗干员",
      "zh": "......好险.......",
      "en": "....That was close….",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "罗德岛干员",
      "zh": "另一批伏兵开始攻击了！",
      "en": "Another wave incoming!",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "——你们不可能活着出去。",
      "en": "—You’re not leaving here alive",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "想都别想！",
      "en": "Don’t even think about it",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "这个数量和配备......",
      "en": "These numbers and equipment",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "难道整合运动早就暗中军事化了？",
      "en": "Has the Reunion Movement already militarized surreptitiously? ",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "博士，你能跑起来吗？",
      "en": "Doctor, can you run?",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "嗯，我明白的。",
      "en": "Yes, I understand",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "那么就——",
      "en": "Well then, let’s—",
      "jp": "",
      "kr": ""
    },
    {
      "ch": "阿米娅",
      "zh": "全小队，全力突破！！",
      "en": "All squad members, all-out break through!",
      "jp": "",
      "kr": ""
    }
  ]
}
