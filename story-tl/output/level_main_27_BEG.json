  "messages": [
    {
      "zh": "第二十七关（前）",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "{@nickname}博士......",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "我......我不是......我......",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "雾散了——",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "近卫局发来通讯，他们发现了更多整合运动的踪迹。",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "阿米娅——",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "......"
    },
    {
      "ch": "星熊",
      "zh": "——你脸色不太好。",
      "tl": ""
    },
    {
      "ch": "PLAYER",
      "zh": "交给我",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "——就交给你了，{@nickname}博士。",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "最好能在五分钟内出发。",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "......"
    },
    {
      "ch": "阿米娅",
      "zh": "——{@nickname}博士——",
      "tl": ""
    },
    {
      "ch": "PLAYER",
      "zh": "询问阿米娅是否在回忆过去;询问阿米娅是否在动摇",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "啊......？我......",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "博士......我不想......我不想的......",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "我......我不知道......",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "我不能说......但我......我很害怕......",
      "tl": ""
    },
    {
      "ch": "PLAYER",
      "zh": "感谢阿米娅保护了自己",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "——————"
    },
    {
      "ch": "阿米娅",
      "zh": "......不用谢。这是我应该做的。",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "我明白了——————",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "我会一直保护你的，博士。",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "我保证。",
      "tl": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "你们先走！",
      "tl": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "我得去......得去把碎骨救回来！",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "怎么会——",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "整合运动......折回来了？他们要做什么——？！",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "星熊警官！快回撤！",
      "tl": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "掩护！掩护我！",
      "tl": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "那个女人......不行，那个女人的盾，我们越不过去！",
      "tl": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "快！迂回！趁他们没反应过来......",
      "tl": ""
    },
    {
      "ch": "整合运动成员",
      "zh": "先把碎骨抢回来！！",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "罗德岛！他们转移了！！",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "星熊警官！小心右边！",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "——嗯——？",
      "tl": ""
    },
    {
      "ch": "W",
      "zh": "呼呼，用盾挡开了？有些意思——",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "整合运动的偷袭？！",
      "tl": ""
    },
    {
      "ch": "W",
      "zh": "当然不止是偷袭哦~",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "啧——哪来的混蛋！",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "阿米娅！我遭到远程火力的压制！",
      "tl": ""
    },
    {
      "ch": "星熊",
      "zh": "整合运动正朝你们的方向快速移动！",
      "tl": ""
    },
    {
      "ch": "W",
      "zh": "唔，这面盾还挺厚实的。我似乎有点自找麻烦呢......",
      "tl": ""
    },
    {
      "ch": "W",
      "zh": "算了，就当我帮整合运动一点小忙吧。",
      "tl": ""
    },
    {
      "ch": "W",
      "zh": "要让我看到有趣的事情哦。可别让我失望~",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "啧——是切城那个W！陈长官他们被她甩掉了吗？！",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "小心突袭！迅速援护星熊警官！",
      "tl": ""
    }
  ]
}