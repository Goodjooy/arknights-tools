    {
      "ch": "小女孩",
      "zh": "————————————。",
      "tl": ""
    },
    {
      "ch": "小女孩",
      "zh": "——————————————————————————！",
      "tl": ""
    },
    {
      "ch": "小女孩",
      "zh": "————————！",
      "tl": ""
    },
    {
      "ch": "另一个少女的声音",
      "zh": "——————。",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "......"
    },
    {
      "ch": "陈",
      "zh": "......"
    },
    {
      "ch": "阿米娅",
      "zh": "陈警司 ......",
      "tl": ""
    },
    {
      "ch": "陈",
      "zh": "............"
    },
    {
      "ch": "阿米娅",
      "zh": "......"
    },
    {
      "ch": "阿米娅",
      "zh": "陈警司，你来这里是......",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "我以为......",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "......"
    },
    {
      "ch": "阿米娅",
      "zh": "......"
    },
    {
      "ch": "阿米娅",
      "zh": "陈警司......",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "她刚才的眼神，我从没有见过。",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "她......",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "在恐惧着什么。",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "也是。我从来都不是一个人。",
      "tl": ""
    },
    {
      "ch": "阿米娅",
      "zh": "......也许这些孩子，真的会走上不一样的道路呢。",
      "tl": ""
    }
  ]
